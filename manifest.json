{
  "manifest_version": 2,

  "name": "Git branch name generator",
  "description": "Convert your Github issue title to branch name",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["getPageSource.js"]
    }
  ]
}
